{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "array-type": false,
        "interface-name": false,
        "jsx-no-lambda": false,
        "member-access": false,
        "no-angle-bracket-type-assertion": false,
        "no-any": true,
        "no-console": false,
        "no-var-keyword": true,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": false,
        "semicolon": false,
        "trailing-comma": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}